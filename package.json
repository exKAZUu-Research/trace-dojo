{
  "name": "trace-dojo",
  "version": "0.0.0-semantically-released",
  "repository": "github:exKAZUu-Research/trace-dojo",
  "license": "Apache-2.0",
  "author": "Kazunori Sakamoto",
  "scripts": {
    "build": "wb buildIfNeeded -c 'yarn run build/core'",
    "build/core": "next build",
    "cleanup": "yarn format && yarn lint-fix",
    "db-help": "wb prisma --help",
    "db-migrate-apply": "wb prisma migrate",
    "db-migrate-create": "wb prisma migrate-dev",
    "db-reset": "wb prisma reset",
    "db-view": "wb prisma studio",
    "db-view-restored": "wb prisma studio --restored",
    "deploy": "wb optimizeForDockerBuild --outside && fly deploy -c fly-${WB_ENV}.toml --build-arg ARCH=amd64 --build-arg WB_ENV=${WB_ENV} --build-arg WB_VERSION=${WB_VERSION}",
    "deploy/ci-setup": "echo ${GCP_SA_KEY_JSON} > gcp-sa-key.json",
    "format": "sort-package-json && yarn prettify",
    "gen-code": "prisma generate && chakra-cli tokens src/theme.ts",
    "postinstall": "husky || true",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky || true",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "wb start",
    "start-docker": "wb start --mode docker",
    "start-stg": "wb start --mode staging",
    "test": "wb test",
    "test-unit": "wb test --unit",
    "test/ci-setup": "wb prisma reset",
    "typecheck": "wb typecheck"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@chakra-ui/next-js": "2.4.1",
    "@chakra-ui/react": "2.10.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@prisma/client": "5.20.0",
    "@tanstack/react-query": "5.59.0",
    "@trpc/client": "11.0.0-rc.561",
    "@trpc/next": "11.0.0-rc.561",
    "@trpc/react-query": "11.0.0-rc.561",
    "@trpc/server": "11.0.0-rc.561",
    "@willbooster/shared-lib-react": "3.2.9",
    "@willbooster/wb": "8.0.2",
    "build-ts": "13.1.8",
    "dayjs": "1.11.13",
    "dotenv-cli": "7.4.2",
    "fast-deep-equal": "3.1.3",
    "framer-motion": "11.11.1",
    "immer": "10.1.1",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.1.0",
    "next": "14.2.14",
    "nextjs-cors": "2.2.0",
    "nextjs-toploader": "3.7.15",
    "pino": "9.4.0",
    "pino-pretty": "11.2.2",
    "pm2": "5.4.2",
    "prisma": "5.20.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-idle-timer": "5.7.2",
    "react-syntax-highlighter": "15.5.0",
    "seedrandom": "3.0.5",
    "superjson": "2.2.1",
    "supertokens-auth-react": "0.47.1",
    "supertokens-node": "20.1.3",
    "supertokens-web-js": "0.13.0",
    "use-context-selector": "2.0.0",
    "use-immer": "0.10.0",
    "usehooks-ts": "3.1.0",
    "uuid": "10.0.0",
    "zod": "3.23.8",
    "zod-form-data": "2.0.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "2.5.4",
    "@types/cookie": "0.6.0",
    "@types/eslint": "9.6.1",
    "@types/jsonwebtoken": "9.0.7",
    "@types/micromatch": "4.0.9",
    "@types/node": "22.7.4",
    "@types/react-dom": "18.3.0",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/seedrandom": "3.0.8",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "@willbooster/eslint-config-next": "1.2.0",
    "@willbooster/prettier-config": "9.1.2",
    "concurrently": "9.0.1",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sort-class-members": "1.20.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "55.0.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "micromatch": "4.0.8",
    "open-cli": "8.0.0",
    "pinst": "3.0.0",
    "prettier": "3.3.3",
    "semantic-release": "24.1.2",
    "sort-package-json": "2.10.1",
    "typescript": "5.6.2",
    "vitest": "2.1.2",
    "wait-on": "8.0.1"
  },
  "packageManager": "yarn@4.5.0",
  "publishConfig": {
    "access": "public"
  },
  "prisma": {
    "seed": "build-ts run prisma/seed.mts"
  }
}
