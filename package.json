{
  "name": "trace-dojo",
  "version": "0.0.0-semantically-released",
  "repository": "github:exKAZUu-Research/trace-dojo",
  "license": "Apache-2.0",
  "author": "Kazunori Sakamoto",
  "scripts": {
    "build": "wb buildIfNeeded -c 'yarn run build/core'",
    "build/core": "next build",
    "cleanup": "yarn format && yarn lint-fix",
    "db-help": "wb prisma --help",
    "db-migrate-apply": "wb prisma migrate",
    "db-migrate-create": "wb prisma migrate-dev",
    "db-reset": "wb prisma reset",
    "db-view": "wb prisma studio",
    "db-view-restored": "wb prisma studio --restored",
    "deploy": "wb optimizeForDockerBuild --outside && fly deploy -c fly-${WB_ENV}.toml --build-arg ARCH=amd64 --build-arg WB_ENV=${WB_ENV} --build-arg WB_VERSION=${WB_VERSION}",
    "deploy/ci-setup": "echo ${GCP_SA_KEY_JSON} > gcp-sa-key.json",
    "format": "sort-package-json && yarn prettify",
    "gen-code": "prisma generate && chakra-cli tokens src/theme.ts",
    "postinstall": "husky || true",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "husky || true",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "next dev",
    "start-prod": "next start",
    "test": "vitest tests",
    "test/ci-setup": "wb prisma reset",
    "typecheck": "wb typecheck"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@chakra-ui/next-js": "2.2.0",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.13.0",
    "@emotion/styled": "11.13.0",
    "@prisma/client": "5.18.0",
    "@tanstack/react-query": "5.52.0",
    "@willbooster/shared-lib-react": "3.2.8",
    "@willbooster/wb": "7.8.14",
    "dotenv": "16.4.5",
    "framer-motion": "11.3.28",
    "next": "14.2.5",
    "nextjs-cors": "2.2.0",
    "pino": "9.3.2",
    "pino-pretty": "11.2.2",
    "pm2": "5.4.2",
    "prisma": "5.18.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-idle-timer": "5.7.2",
    "react-syntax-highlighter": "15.5.0",
    "seedrandom": "3.0.5",
    "supertokens-auth-react": "0.45.1",
    "supertokens-node": "20.0.2",
    "supertokens-web-js": "0.13.0",
    "usehooks-ts": "3.1.0",
    "uuid": "10.0.0",
    "zod": "3.23.8",
    "zod-form-data": "2.0.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "2.4.1",
    "@types/cookie": "0.6.0",
    "@types/eslint": "9.6.0",
    "@types/micromatch": "4.0.9",
    "@types/node": "22.4.2",
    "@types/react-dom": "18.3.0",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/seedrandom": "3.0.8",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.2.0",
    "@typescript-eslint/parser": "8.2.0",
    "@willbooster/eslint-config-next": "1.2.0",
    "@willbooster/prettier-config": "9.1.2",
    "build-ts": "13.1.8",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sort-class-members": "1.20.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "55.0.0",
    "husky": "9.1.5",
    "lint-staged": "15.2.9",
    "micromatch": "4.0.7",
    "pinst": "3.0.0",
    "prettier": "3.3.3",
    "sort-package-json": "2.10.0",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "packageManager": "yarn@4.4.0",
  "publishConfig": {
    "access": "public"
  },
  "prisma": {
    "seed": "build-ts run prisma/seed.mts"
  }
}
